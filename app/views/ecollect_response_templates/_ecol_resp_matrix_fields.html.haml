.nested-fields
  .form-main{:style=>'width: 100%'}
    - if f.object.new_record?
      = f.hidden_field :created_by,value: current_user.id
    - if f.object.id.present?
      = f.hidden_field :updated_by,value: current_user.id
    .control-group
      = f.label :action, "Type", :class => 'control-label', :class => 'control-label required'
      = f.select :action, [ 'PASS', 'REJECT'], {:include_blank => 'Select Action'}
  .form-main{:style=>'width: 50%'}  
    .control-group
      = f.label :key1, "Key1", :class => 'control-label', :class => 'control-label required'
      = f.text_field :key1,:class => 'key1'
    .control-group
      = f.label :key2, "Key2", :class => 'control-label', :class => 'control-label required'
      = f.text_field :key2,:class => 'key2'
