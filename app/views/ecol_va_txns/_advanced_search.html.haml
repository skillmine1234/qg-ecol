= search_form_for @searcher, {defaults: local_assigns[:defaults]} do |f|
  = f.standard_fields
  = f.input :account_no
  = f.input :hold_no
  = f.input :account_balance
  = f.input :hold_amount
  = f.input :txn_amount, as: :between
  = f.input :txn_timestamp, as: :between
  = f.action_buttons
= pagination_links(@records)